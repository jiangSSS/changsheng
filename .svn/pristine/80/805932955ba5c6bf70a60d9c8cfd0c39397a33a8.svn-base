// 加载
let loading;

function startLoading() {
  loading = Loading.service({
    lock: true,
    text: "拼命加载中...",
    spinner: "my-el-custom-spinner",
    background: "rgba(255,255,255,0)"
  });
}

function endLoading() {
  loading.close();
}
axios.interceptors.request.use(
  function (config) {
    startLoading();
    if (localStorage.getItem("token")) {
      config.headers.Authorization = localStorage.getItem("token");
    } else {
      let ahths = window.location.href
      let isAuth = ahths.indexOf('auth=')
      let auths
      if (isAuth > 0) {
        auths = ahths.match(/auth=(\S*)/)[1];
      } else {
        auths = ''
      }
      config.headers.Authorization = auths;
    }
    return config;
  },
  function (error) {
    return Promise.reject(error);
  }
);

axios.interceptors.response.use(
  function (response) {
    endLoading();
    // console.log("responsData", response.data);
    return response;
  },
  function (error) {
    if (error.response) {
      switch (error.response.status) {
        case 401:
          localStorage.removeItem("token");
          router.replace({
            path: "/" //跳转首页
          });
      }
    }
    return Promise.reject(error);
  }
);

const xhr = {
  post(url, data, config) {
    return new Promise((resolve, reject) => {
      axios
        .post(url, data, config)
        .then(res => {
          resolve(res.data);
        })
        .catch(err => {
          reject(err);
        });
    });
  }
};
export const $axios = xhr;
export function post(service, params) {
  return new Promise((resolve, reject) => {
    var target = {
      headers: {
        transNo: generateUUID(),
        salechnl: "00",
        transCode: "0",
        requestDate: getDate(),
        requestTime: getTime(),
        standbyflag1: "0",
        standbyflag2: "0",
        standbyflag3: "0"
      }
    };
    axios
      .post(service, Object.assign(target, params))
      .then(result => {
        endLoading();
        resolve(result);
        // window.vm.$store.commit('isScroll',false);
        // console.log(store)
      })
      .catch(error => {
        endLoading();
        // window.vm.$store.commit('isScroll',false);
        console.log(error);
        // alert('系统繁忙，请稍后重试！')

        reject(result);
      });
  });
}

