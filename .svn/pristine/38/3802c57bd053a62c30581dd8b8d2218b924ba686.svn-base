<template>
  <div class="indexDatabaseForm">
    <el-collapse v-model="activeName">
      <el-collapse-item name="1">
        <template slot="title">
          统计信息
          <i
            class="header-icon el-icon-caret-bottom"
            style="margin: 0 8px 0 auto;font-size: 30px;"
          ></i>
        </template>
        <el-form
          ref="listForm"
          :model="listForm"
          v-model="labelPosition"
          label-width="34%"
          class="demo-ruleForm"
          :rules="rules"
        >
          <el-row>
            <el-col :span="8">
              <div class="grid-content">
                <el-form-item label="机构" prop="filingAgency">
                  <el-select
                    v-model="listForm.filingAgency"
                    clearable
                    placeholder="请选择"
                    :disabled="agency"
                  >
                    <el-option
                      v-for="item in agencyList"
                      :key="item.filingAgency"
                      :label="item.agencyName"
                      :value="item.filingAgency"
                    ></el-option>
                  </el-select>
                </el-form-item>
              </div>
            </el-col>
            <el-col :span="8">
              <div class="grid-content">
                <el-form-item label="统计起期" prop="validstartdate">
                  <el-date-picker
                    v-model="listForm.validstartdate"
                    type="date"
                    placeholder="选择日期"
                    value-format="yyyy-MM-dd"
                  ></el-date-picker>
                </el-form-item>
              </div>
            </el-col>
            <el-col :span="8">
              <div class="grid-content">
                <el-form-item label="统计止期" prop="validenddate">
                  <el-date-picker
                    v-model="listForm.validenddate"
                    type="date"
                    placeholder="选择日期"
                    value-format="yyyy-MM-dd"
                  ></el-date-picker>
                </el-form-item>
              </div>
            </el-col>
          </el-row>
          <el-row>
            <el-col :span="8">
              <div class="grid-content">
                <!-- class="jiuzhenhosp"
                        v-model="objForm.hospitalname"
                        filterable
                        placeholder="请选择"
                        clearable
                        :filter-method="waitFunc(filtered,400)"
                        @change="changeHospital(objForm.hospitalname)"
                v-el-select-loadmore="loadmore"-->
                <el-form-item label="营销服务部" prop="agentcom">
                  <el-select
                    v-model="listForm.agentcom"
                    filterable
                    clearable
                    placeholder="请选择"
                    :disabled="agency"
                    @focus="queryMarket"
                    :filter-method="waitFunc(filtered,400)"
                    v-el-select-loadmore="loadmore"
                  >
                    <el-option
                      v-for="item in islist"
                      :key="item.comcode"
                      :label="item.comcode +'-'+item.name"
                      :value="item.comcode"
                    ></el-option>
                  </el-select>
                </el-form-item>
              </div>
            </el-col>
            <el-col :span="8">
              <div class="grid-content">
                <el-form-item label="渠道" prop="salechnl">
                  <el-select
                    multiple
                    collapse-tags
                    v-model="listForm.salechnl"
                    clearable
                    placeholder="请选择"
                    :disabled="agency"
                  >
                    <el-option
                      v-for="item in ChannelList"
                      :key="item.code"
                      :label="item.code + '-'+ item.codename"
                      :value="item.code"
                    ></el-option>
                  </el-select>
                </el-form-item>
              </div>
            </el-col>
          </el-row>
          <el-row style="text-align:right;padding-right:30px;">
            <el-button type="primary" round @click="targeReportFormGet('listForm')">打印指标库报表</el-button>
          </el-row>
        </el-form>
      </el-collapse-item>
    </el-collapse>
  </div>
</template>
<script>
import axios from "axios";
import { post, service } from "../../utils/request.js";
// import { getCodeArr } from "../../utils/service";
export default {
  name: "indexDatabaseForm",
  directives: {
    "el-select-loadmore": {
      bind(el, binding) {
        const elss = el.querySelector(
          ".el-select-dropdown .el-select-dropdown__wrap"
        );
        elss.addEventListener("scroll", function() {
          const condition =
            this.scrollHeight - this.scrollTop - 1 <= this.clientHeight;
          if (condition) {
            binding.value();
          }
        });
      }
    }
  },
  data() {
    return {
      agency: true,
      listForm: {
        filingAgency: "",
        validenddate: "",
        validstartdate: "",
        salechnl: [],
        agentcom: ""
      },
      activeName: ["1"],
      labelPosition: "right",
      agencyList: [],
      ChannelList: [],
      islist: [],
      startPage:0,
      hosName: "",
      rules: {
        validstartdate: [
          {
            required: true,
            message: "请选择统计起期",
            trigger: "blur"
          }
        ],
        validenddate: [
          {
            required: true,
            message: "请选择统计止期",
            trigger: "blur"
          }
        ]
      }
    };
  },
  inject: ["reload"],
  created() {
    this.getCodeList();
    this.pageQuery();
    // this.queryMarket();
  },
  mounted() {
    this.$watermark.set(
      localStorage.getItem("userCode"),
      localStorage.getItem("userName")
    );
  },
  methods: {
    // 营销服务部
    queryMarket() {
      this.hosName = '';
      post(service.queryMarket, {
        pagestart: 0,
        pageend: 100,
        likename: ""
      }).then(res => {
        this.islist = res.data.bodys;
      });
    },
    loadmore() {
      this.startPage += 100;
      post(service.queryMarket, {
        pagestart: this.startPage,
        pageend: 100,
        likename: this.hosName || ""
      }).then(res => {
        this.islist = [...this.islist, ...res.data.bodys];
      });
    },
    waitFunc(func, wait) {
      let timer = 0;
      return function(...args) {
        if (timer) clearTimeout(timer);
        timer = setTimeout(() => {
          func.apply(this, args);
        }, wait);
      };
    },
    // 就诊医院下拉框（可搜索）
    filtered(val) {
      this.startPage = 0;
      this.hosName = val;
      if (val != "") {
        post(service.queryMarket, {
          pagestart: this.startPage,
          pageend: 100,
          likename: val
        }).then(res => {
          this.islist = res.data.bodys;
        });
      }
    },
    // 渠道
    pageQuery() {
      post(service.pageQuery, {
        bodys: {
          codetype: "formsalechnl"
        }
      }).then(res => {
        this.ChannelList = res.data.bodys;
      });
    },
    // 机构
    getCodeList: function() {
      post(service.mngComQuery, {
        bodys: {
          comCode: localStorage.getItem("comCode")
        }
      }).then(res => {
        if (res.data.header.code === "200") {
          this.agencyList = res.data.bodys;
          if (this.agencyList.length > 0) {
            this.agency = false;
          } else {
            this.agency = true;
          }
        }
      });
    },
    targeReportFormGet(formName) {
      console.log(this.listForm.salechnl)
      this.$refs[formName].validate(valid => {
        if (!valid) {
          return false;
        } else {
          if (
            (new Date(this.listForm.validenddate).getTime() -
              new Date(this.listForm.validstartdate).getTime()) /
              1000 <
            0
          ) {
            this.$message.error("统计起期不能大于统计止期！");
            return;
          }
          post(service.targeReportFormGet, {
            bodys: {
              startdate: this.listForm.validstartdate,
              enddate: this.listForm.validenddate,
              managecom: this.listForm.filingAgency,
              agentcom: this.listForm.agentcom,
              salechnl: this.listForm.salechnl.length != 0 ? `'` + this.listForm.salechnl.join(`','`) + `'` : ""
            }
          }).then(res => {
            if (res.data.bodys && res.data.bodys.filepath) {
              window.open(res.data.bodys.filepath);
            } else {
              // this.$message.error(res.data.header.message);
              this.$message.error("理赔报表无数据，请确认查询条件！");
            }
          });
        }
      });
    }
  }
};
</script>
