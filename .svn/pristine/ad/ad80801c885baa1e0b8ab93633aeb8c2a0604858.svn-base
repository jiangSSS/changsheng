                center: this.pulicList[key].rgtno,
                right: "，立案时间：" + this.pulicList[key].rgtconfdate
              });
              break;
            case "waitApprove":
              this.strList.push({
                otherno: this.pulicList[key].customerno,
                current: this.pulicList[key].mark,
