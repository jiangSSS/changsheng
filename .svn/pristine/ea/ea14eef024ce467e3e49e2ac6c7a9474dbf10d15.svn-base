    var isMobileNumber = (rule, value, callback) => {
      if (!value) {
        return new Error("请输入电话");
      } else {
        var reg = /^\d+$|^\d+[.]?\d+$/;
        if (!reg.test(value)) {
          callback(new Error("电话号码不合法!"));
        } else {
          if (value.substr(0, 1) == 1) {
            // debugger;
            if (value.length != 11) {
              callback(new Error("电话号码不合法!"));
            } else {
              callback();
            }
          } else {
            callback();
          }
        }
      }
    };
